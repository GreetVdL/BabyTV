{
  "name": "starter-kit-parcel",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "npm i -D parcel@next",
    "start": "parcel serve ./src/index.html --open",
    "clean": "rm -rf ./dist ./.parcel-cache ./out",
    "build": "npm run clean && parcel build ./src/index.html --dist-dir ./out --no-source-maps",
    "serve": "live-server ./out",
    "deploy": "surge ./out --domain https://testvangreet.surge.sh"
  },
